#=========================================
#config
#=========================================
# data driver config (please do not modify without guide of developer)
database.driver.class=com.mysql.jdbc.Driver
database.driver.scheme=jdbc:mysql
database.driver.dialect=org.hibernate.dialect.MySQL5InnoDBDialect
database.driver.showSQL=false

# database server config
database.host.name=localhost
database.host.port=3306
database.scheme.name=authentication
database.authen.username=<%=@db_name%>
database.authen.password=<%=@db_password%>
database.pool.initSize=4
database.pool.maxIdle=8
database.pool.maxActive=16
database.params.queryString=autoReconnect=false&useUnicode=true&characterEncoding=UTF-8
