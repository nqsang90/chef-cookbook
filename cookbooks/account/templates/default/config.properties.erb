keytrust=/WEB-INF/keytrust.properties
ip=<%=@host_xsecd%>

#xsecd config
# development:com.mbv.services.test.FakeXsecdService
# production:com.mbv.services.xsecd.XsecdService 

xsecd.class=com.mbv.services.test.FakeXsecdService
xsecd.url=http://${ip}:9999
#xsecd.url=http://192.168.254.60:9999
xsecd.timeout=30000
xsecd.pool.initSize=2
xsecd.pool.maxSize=16

#memcached config
memcache.server=localhost:11211

#otpcmd config
otpcmd.connection.host=<%=@host_xsecd%>
otpcmd.connection.port=23456
otpcmd.pool.initSize=1
otpcmd.pool.maxSize=4

#account
#url deploy account
account.url.deploy=https://<%=node['ipaddress']%>:<%=node['account']['port']%>/account

#mca user
mca.user.url=https://<%=node['ipaddress']%>:<%=node['account']['port']%>/user/index.htm



