# database
#=========================================
# database server config
database.host=<%=@db_host%>
database.port=<%=@db_port%>

jdbc.username	 = <%=@db_username%>
jdbc.password	 = <%=@db_password%>
jdbc.schema     = <%=@db_schema%>
jdbc.driver	 = com.mysql.jdbc.Driver
jdbc.url	= ${database.driver.scheme}://${database.host}:${database.port}/${jdbc.schema}?useUnicode=true&characterEncoding=utf-8&zeroDateTimeBehavior=convertToNull

database.driver.dialect=org.hibernate.dialect.MySQL5InnoDBDialect
database.driver.showSQL=false
database.driver.scheme=jdbc:mysql
