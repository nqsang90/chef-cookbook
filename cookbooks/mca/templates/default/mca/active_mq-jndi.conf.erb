java.naming.factory.initial = org.apache.activemq.jndi.ActiveMQInitialContextFactory

# use the following property to configure the default connector
java.naming.provider.url = tcp://<%=@host_active_mq%>:61616

# use the following property to specify the JNDI name the connection factory
# should appear as. 
#connectionFactoryNames = connectionFactory, queueConnectionFactory, topicConnectionFactry

# register some queues in JNDI using the form
# queue.[jndiName] = [physicalName]

# queued to aggregate queued message
queue.QueuedMessage.In = Queue.QueuedMessage.In

# queued to notify queued message
queue.QueuedMessage.Out = Queue.QueuedMessage.Out

# queued to forward XN Sms command
# queue.QueuedMessage.Sms.XN = Queue.QueuedMessage.Sms.XN
topic.QueuedMessage.Sms.XN = Topic.QueuedMessage.Sms.XN

# queued to forward MBV POS Sms command
queue.QueuedMessage.Sms.MBV_POS = Queue.QueuedMessage.Sms.MBV_POS
#topic.QueuedMessage.Sms.MBV_POS = Topic.QueuedMessage.Sms.MBV_POS

# register some topics in JNDI using the form
# topic.[jndiName] = [physicalName]
topic.MyTopic = example.MyTopic
