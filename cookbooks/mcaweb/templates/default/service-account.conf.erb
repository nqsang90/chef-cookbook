# Account client config

# URL to account web system (for admin authentication)
account.url = http://<%=@host_account%>:8080/account
account.url.internal = http://<%=@host_account%>:8080/account

session.conn.url=${account.url.internal}/soap/Session
session.conn.timeout=10000

session.pool.init=1
session.pool.max=16
