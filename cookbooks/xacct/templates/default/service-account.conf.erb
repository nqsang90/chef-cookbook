# account deployment config

# URL to account web system (for admin authentication)
#account.url = https://<%=@gateway_account%>/account
account.url = http://<%=@host_account%>:<%=@port_account%>/account
account.url.internal = http://<%=@host_account%>:<%=@port_account%>/account

session.conn.url=${account.url}/soap/Session
session.conn.timeout=10000

session.pool.init=1
session.pool.max=16
